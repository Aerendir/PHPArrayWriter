{
    "name": "serendipity_hq/php_array_writer",
    "description": "A class to read and write PHP arrays.",
    "type": "library",
    "license": "MIT",
    "keywords": ["array"],
    "homepage": "https://github.com/Aerendir/PHPArrayWriter",
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/property-access": "^2.8||^3.0||^4.0||^5.0",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.3",
        "drenso/phan-extensions": "3.1.0",
        "phan/phan": "^2.4.6 || ^3.0.0",
        "phpmetrics/phpmetrics": "^2.5",
        "psalm/plugin-phpunit": "0.10.1",
        "rector/rector": "^0.6.11 || ^0.7.0",
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^2.8||^3.0||^4||^5",
        "vimeo/psalm": "3.11.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SHQ\\Component\\ArrayWriter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SHQ\\Component\\ArrayWriter\\Tests\\": "tests"
        }
    }
}
