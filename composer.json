{
    "name": "serendipity_hq/php_array_writer",
    "description": "A class to read and write PHP arrays.",
    "type": "library",
    "license": "MIT",
    "keywords": ["array"],
    "homepage": "https://github.com/Aerendir/PHPArrayWriter",
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/property-access": "^3.0||^4.0||^5.0",
        "thecodingmachine/safe": "^0.1|^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^3.0||^4.0||^5.0",
        "thecodingmachine/phpstan-safe-rule": "@stable"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SHQ\\Component\\ArrayWriter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SHQ\\Component\\ArrayWriter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "bin": "echo 'bin not installed'",
        "post-install-cmd": ["@composer bin all install --ansi"],
        "post-update-cmd": ["@composer bin all update --ansi"]
    }
}
