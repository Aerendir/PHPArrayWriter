{
    "name": "serendipity_hq/php_array_writer",
    "description": "A class to read and write PHP arrays.",
    "type": "library",
    "license": "MIT",
    "keywords": ["array"],
    "homepage": "https://github.com/Aerendir/PHPArrayWriter",
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/property-access": "^3.0||^4.0||^5.0",
        "thecodingmachine/safe": "^0.1|^1.0"
    },
    "require-dev": {
        "drenso/phan-extensions": "@stable",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phan/phan": "^3.0",
        "phpstan/phpstan": "^0.12.32",
        "phpstan/phpstan-phpunit": "@stable",
        "phpunit/phpunit": "^8.5",
        "psalm/plugin-phpunit": "@stable",
        "rector/rector": "^0.7.43",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^6.0",
        "symfony/var-dumper": "^3.0||^4.0||^5.0",
        "thecodingmachine/phpstan-safe-rule": "@stable",
        "vimeo/psalm": "^3.12"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SHQ\\Component\\ArrayWriter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SHQ\\Component\\ArrayWriter\\Tests\\": "tests"
        }
    }
}
