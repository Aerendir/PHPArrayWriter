{
    "name": "serendipity_hq/component-array-writer",
    "description": "A class to read and write PHP arrays.",
    "type": "library",
    "license": "MIT",
    "keywords": ["array"],
    "homepage": "https://github.com/Aerendir/component-array-writer",
    "support": {
        "issues": "https://github.com/Aerendir/component-array-writer/issues"
    },
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "aerendir@serendipityhq.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "symfony/property-access": "^3.4|^4.4|^5.1",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^3.4|^4.4|^5.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SHQ\\Component\\ArrayWriter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SHQ\\Component\\ArrayWriter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "bin": "echo 'bin not installed'",
        "post-install-cmd": ["@composer bin all install --ansi"],
        "post-update-cmd": ["@composer bin all update --ansi"]
    }
}
